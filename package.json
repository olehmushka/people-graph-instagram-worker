{
  "name": "people-graph-instagram-worker",
  "version": "0.0.0",
  "main": "dist/index.js",
  "author": "Oleh Mushka <olegamysk@gmail.com>",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=12.13"
  },
  "scripts": {
    "build": "yarn clear && copyfiles swagger.yml dist && echo Using TypeScript && tsc --version && tsc --pretty",
    "clear": "rimraf dist",
    "lint": "eslint './src/**/*.ts'",
    "lint:fix": "eslint './src/**/*.ts' --fix",
    "lint:report": "npm run lint -- -f node_modules/eslint-html-reporter/reporter.js -o reports/linter/index.html",
    "format": "prettier --write \"src/**/*.ts\"",
    "prestart": "yarn format && yarn build",
    "start": "node -r dotenv/config .",
    "start:dev": "nodemon -e ts,json -i dist -i tests -i reports -x \"npm start\" | pino-pretty -t HH:MM:ss",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@hapi/joi": "17.1.1",
    "@hapi/joi-date": "2.0.1",
    "cheerio": "1.0.0-rc.3",
    "dotenv": "8.1.0",
    "lodash": "4.17.15",
    "moment": "2.25.3",
    "pino": "5.13.5",
    "puppeteer": "4.0.0",
    "uuid": "8.0.0"
  },
  "devDependencies": {
    "@types/chai": "4.2.1",
    "@types/cheerio": "0.22.18",
    "@types/faker": "4.1.9",
    "@types/hapi__joi": "17.1.0",
    "@types/jest": "24.0.15",
    "@types/lodash": "4.14.138",
    "@types/node": "12.6.3",
    "@types/pino": "5.8.10",
    "@types/puppeteer": "3.0.0",
    "@types/uuid": "7.0.3",
    "@typescript-eslint/eslint-plugin": "2.23.0",
    "@typescript-eslint/parser": "2.23.0",
    "chai": "4.2.0",
    "chai-asserttype": "1.0.5",
    "copyfiles": "2.1.1",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-html-reporter": "0.7.4",
    "eslint-plugin-chai-friendly": "0.5.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-mocha-no-only": "1.1.0",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-unused-imports": "0.1.3",
    "faker": "4.1.0",
    "jest": "24.9.0",
    "jest-junit": "9.0.0",
    "pino-pretty": "3.2.1",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "typescript": "3.8.3"
  }
}
